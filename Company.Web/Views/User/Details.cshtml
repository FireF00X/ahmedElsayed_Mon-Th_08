<h1>User Details</h1>

<form>
    <div class="form-group">
        <label class="col-form-label" asp-for="UserName"></label>
        <input type="text" asp-for="UserName" class="form-control" readonly />
        <span asp-validation-for="UserName" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label class="col-form-label" asp-for="Email"></label>
        <input type="text" asp-for="Email" class="form-control" readonly />
        <span asp-validation-for="Email" class="text-danger"></span>
    </div>
    
   
    <a asp-action="Index" class="btn btn-light">Back To List</a>
</form>